<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seekersoftvendingapp">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SeekersoftApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SettingActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".test.TestCardReadActivity" />
        <activity android:name=".database.dao.TestNoteDaoActivity" />
        <activity android:name=".database.dao.TestAdminCardDaoActivity" />

        <activity android:name=".database.rxdao.TestNoteRXDaoActivity" />
        <activity android:name=".image.TestFrescoActivity" />
        <activity android:name=".network.TestNetworkActivity" />
        <activity android:name=".test.TestVendingActivity" />
        <activity android:name=".test.TestStoreActivity" />


        <activity android:name=".StartAppActivity" />
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTop" />
        <activity android:name=".TakeOutInsertNumActivity" />
        <activity android:name=".TakeOutCardReadActivity" />


        <activity android:name=".BorrowInsertNumActivity" />
        <activity android:name=".BorrowCardReadActivity" />


        <activity android:name=".ReturnInsertNumActivity" />
        <activity android:name=".ReturnCardReadActivity" />


        <activity android:name=".HandleResultActivity" />
        <activity android:name=".ManagerGoodsActivity" />


    </application>

</manifest>